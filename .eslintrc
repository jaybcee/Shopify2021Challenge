{
  "parser": "babel-eslint",
  "extends": [
  "eslint:recommended",
  "plugin:react/recommended",

],
"ecmaFeatures": {
  "modules": true,
  "spread" : true,
  "restParams" : true
},
"env" : {
  "browser" : true,
  "node" : true,
  "es6" : true
},
"rules" : {
  "no-unused-vars" : 2,
  "no-undef" : 2
},
"parserOptions": {
  "sourceType": "module"
}
}
